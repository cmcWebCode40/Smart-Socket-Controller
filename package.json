{
  "name": "SmartHomeEnergyMgtSys",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "ts:check": "yarn tsc --noemit",
    "clean:android:gradlew": "cd android && ./gradlew clean && cd ..",
    "android:prod:build": "npm run clean:android:gradlew && npx react-native build-android --mode=release",
    "android:staging:build": "npm run clean:android:gradlew && cd android && ./gradlew assembleRelease && cd ..",
    "clean:ios:pods": "cd ios && rm -rf ./ios/Pods/** && npx pod-install && cd .."
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "babel-plugin-module-resolver": "^5.0.2",
    "react": "18.2.0",
    "react-native": "0.74.2",
    "react-native-ble-manager": "^11.5.5",
    "react-native-circular-progress": "^1.4.0",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-gifted-charts": "^1.4.10",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "^3.12.0",
    "react-native-safe-area-context": "^4.10.4",
    "react-native-screens": "^3.31.1",
    "react-native-svg": "^15.3.0",
    "text-decoding": "^1.0.0",
    "text-encoding": "^0.7.0"
